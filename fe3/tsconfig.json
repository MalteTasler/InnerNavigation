{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "checkJs": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "moduleResolution": "Node",
    "target": "ES2020",
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true
  }
}
